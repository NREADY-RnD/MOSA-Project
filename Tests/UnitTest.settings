Optimizations:
	Basic: true
	SSA: true
	SCCP: true
	ValueNumbering: true
	LongExpansion: true
	BitTracker: true
	LoopInvariantCodeMotion: true
	Platform: true
	Inline: true
		Maximum: 12
		AggressiveMaximum: 24
		ExplicitOnly: true
	TwoPass: true

#Compiler.Multithreading: false
#CompilerDebug.PostlinkHashFile: %DEFAULT%
#CompilerDebug.PrelinkHashFile: %DEFAULT%

#Emulator.Display: on
